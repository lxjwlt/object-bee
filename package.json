    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "mocha": "^3.4.2"
  },
  "dependencies": {
    "clone": "^2.1.1",
    "lodash.isplainobject": "^4.0.6",
